{
  "name": "hotel-price-scraper",
  "version": "1.0.0",
  "description": "A comprehensive TypeScript-based web scraper for tracking hotel prices from WebBeds",
  "main": "dist/server/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server/index.js",
    "dev": "ts-node --project tsconfig.json src/server/index.ts",
    "dev:watch": "nodemon --watch src --ext ts --exec \"ts-node\" src/server/index.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf dist node_modules",
    "prebuild": "npm run lint",
    "type-check": "tsc --noEmit"
  },
  "keywords": ["hotel", "scraper", "price", "webbeds", "typescript", "web-scraping"],
  "author": "waleedqari2",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "puppeteer": "^21.6.1",
    "cheerio": "^1.0.0-rc.12",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
